pipeline {
    agent any
    tools{
        nodejs "NodeJS"
    }
    stages {
        stage('Git Checkout') {
            steps {
               git branch: 'main', url: 'https://github.com/Manja2012/calculatrice.git'
            }
        }
        stage('Install Dependencies') {
            steps {
               bat "npm install"
            }
        }
        stage('Run test') {
            steps {
               bat "npm test"
            }
        }
       
    }
}
